<script>
    jQuery(document).ready(function(){
        //延迟加载图片
        echo.init({
            offset: 300,
            throttle:250,
            debounce:false
        });
        $(".home-nav-inner ul li").mouseenter(function(){
            echo.render();
        });
        $(".home-nav ul li").mouseover(function(){
            echo.render();
        });
        cartCookieList();//刷新购物车页面
        //查询消息各种状态数量
        $.ajax({
            url:  myMessage_statusNum,
            data: null,
            method: 'post',
            dataType: 'json',
            //jsonp: 'callback',
            success: function (data) {
                //进行数据处理
                if (data.isSuccess == false) {
                    $("#allMessage_top").html("( 0 )");

                } else {
                    var arr=data.result;
                    $("#allMessage_top").html("( "+arr.S3+" )");
                }
            }
        });
        //获取优惠券、积分提示红点
        getmy_qd_lb_num_topnav();
    });

    //每日签到&&会员礼包
    function getmy_qd_lb_num_topnav() {
        var url = businessApiPath+'?act=receive_gift&op=receive_gift&op=isno_receive_giftvoucherintegral';
        $.get(url,'',function(data) {
            var json = eval('(' + data + ')');
            if(json.code==0) {
                var hothtml = '<span class="hot"></span>';
                var jsondata = json.data;
                var integral = parseInt(jsondata.integral);//积分
                var vouchernum = parseInt(jsondata.voucher_num);//优惠券
                if( integral != 0 ){
                    $('.tool-myCQ-operation').find('a').eq(4).append(hothtml);
                }
                if( vouchernum > 0 ){
                    $('.tool-myCQ-operation').find('a').eq(5).append(hothtml);
                }
            }else{
                return false;
            }
        });
    }

</script>
